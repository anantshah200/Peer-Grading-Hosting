{
  "name": "pga2",
  "version": "0.1.0",
  "main": "main.js",
  "private": true,
  "babelrc": false,
  "engines": {
    "npm": "8.5.5",
    "node": "16.15.0"
  },
  "scripts": {
    "dev": "set NODE_ENV=development && node ./main.js",
    "build": "next build",
    "start": "NODE_ENV=production node main.js -p $PORT",
    "deploy-production": "NODE_ENV=production next build && NODE_ENV=production node ./main.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles"
  },
  "dependencies": {
    "@keyv/sqlite": "^3.5.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/system": "^4.12.2",
    "aws-sdk": "^2.1131.0",
    "axios": "^0.21.4",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cryptr": "^6.0.3",
    "express": "^4.18.1",
    "formik": "^2.2.9",
    "gmail-send": "^1.8.14",
    "grommet": "^2.23.0",
    "ims-lti": "^3.0.2",
    "js-cookie": "^2.2.1",
    "keyv": "^4.2.2",
    "lodash": "^4.17.21",
    "next": "^9.5.5",
    "next-google-fonts": "^1.3.0",
    "next-server": "^9.0.5",
    "pg": "^8.7.3",
    "pify": "^5.0.0",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^16.14.0",
    "react-markdown": "^5.0.3",
    "remark-gfm": "^1.0.0",
    "sass": "^1.51.0",
    "sequelize": "^6.19.0",
    "styled-components": "^5.3.5",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.10.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/pages/test.js",
      "/pages/api/__tests__/testclient.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "eslint-plugin-react",
      "eslint-plugin-prettier"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0
    }
  }
}
